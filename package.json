  "license": "MIT",
  "scripts": {
    "start": "prismixer && npm run prisma-generate && nx serve",
    "build": "prismixer && nx build",
    "build-dev": "prismixer && nx build",
    "prisma-generate": "prisma generate --schema=\"./apps/fhynix-core-apis/src/app/common/DataStore/prisma/schema.prisma\"",
