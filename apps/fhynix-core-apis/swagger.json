{
  "basePath": "/",
  "info": { "title": "Fhynix", "version": "1.0" },
  "paths": {
    "/contact/me": {
      "get": {
        "security": [{ "Bearer": [] }],
        "operationId": "getUsers",
        "tags": ["user"],
        "description": "Get users objects list",
        "summary": "Get users list",
        "responses": {
          "200": {
            "description": "Success",
            "schema": { "$ref": "#/definitions/getUser" }
          },
          "400": { "description": "Client error and Bad Request" },
          "500": { "description": "Internal Server Error" }
        },
        "produces": ["application/json"],
        "consumes": ["application/json"]
      }
    },
    "/auth/login": {
      "post": {
        "operationId": "login",
        "tags": ["login"],
        "description": "Login through auth code",
        "summary": "Get access token",
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "Login through auth code",
            "required": false,
            "schema": {
              "$ref": "#/definitions/LoginDto"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": { "$ref": "#/definitions/login" }
          },
          "400": { "description": "Client error and Bad Request" },
          "500": { "description": "Internal Server Error" }
        },
        "security": [{ "apiKeyHeader": [] }],
        "produces": ["application/json"],
        "consumes": ["application/json"]
      }
    }
  },
  "tags": [{ "name": "getUser" }],
  "schemes": ["https", "http"],
  "produces": ["application/json"],
  "consumes": ["application/json"],
  "definitions": {
    "getUser": {
      "required": ["accessToken"],
      "properties": {
        "id": {
          "type": "integer",
          "uniqueItems": true
        },
        "email": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "isOnboardingCompleted": {
          "type": "boolean"
        }
      }
    },
    "login": {
      "required": ["authCode", "provider"],
      "properties": {
        "authToken": {
          "type": "string"
        }
      }
    },
    "LoginDto": {
      "type": "object",
      "properties": {
        "authCode": {
          "type": "string"
        },
        "provider": {
          "type": "string"
        }
      }
    }
  },
  "securityDefinitions": {
    "Bearer": {
      "name": "Authorization",
      "in": "header",
      "type": "apiKey",
      "description": "JWT Authorization header"
    }
  },
  "swagger": "2.0"
}
