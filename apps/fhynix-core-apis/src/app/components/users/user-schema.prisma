model Users {
  id  String  @id @default(uuid())
  created_at_utc DateTime? @default(now())
  created_by String?
  updated_at_utc DateTime @updatedAt
  updated_by String?
  is_deleted Boolean @default(false)
  account_id String
  email String
  phone String?
  address String?
  timezone_offset_in_mins Int?
  is_onboarding_completed Boolean
}

model FamilyMembers {
  id  String  @id @default(uuid())
  created_at_utc DateTime? @default(now())
  created_by String?
  updated_at_utc DateTime? @updatedAt
  updated_by String?
  is_deleted Boolean @default(false)
  user_id String
  relationship_id String
  first_name String
  last_name String?
  dob DateTime?
  profile_image String?
  gender String?
  other_info String?
  color String?
  personalities String?
  interests String?
}

model RelationshipsMaster {
  id  String   @id @default(uuid())
  created_at_utc DateTime? @default(now())
  created_by String
  updated_at_utc DateTime @updatedAt
  updated_by String
  is_deleted Boolean @default(false)
  relation String
  relation_type String
  is_visible Boolean
}
